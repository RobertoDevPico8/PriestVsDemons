createlevel=function(ptl,eny,ply)
	foreach(ptl,function(i)
		createportal(i[1],i[2],i[3])
	end)
	
	foreach(eny,function(i)
		add(es,newenemy(i[1]*8,i[2]*8,i[3]))
	end)
	
	p.x=ply.x
	p.y=ply.y		
end

createportal=function(x,y,t)
	local s=17
	if t==2 then
		s=27
	elseif t==3 then
		s=29
	end

	mset(x,y,s)
	mset(x,y+1,s+16)
	mset(x+1,y,s+1)
	mset(x+1,y+1,s+17)
end
