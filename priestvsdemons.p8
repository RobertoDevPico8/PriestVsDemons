-- player --
p = {
	s=1,
	x=8,
	y=8,
	spd=1,
	box={x=0,y=0,w=8,h=8}	
}

p.update = function(this)
	xy = getxy(this)
	
	local collwallx,collwally=
			box_hits(
				{x=this.x+sign(xy.dx)*1,
	 			y=this.y,							
					box=this.box},{0}),
			box_hits(
				{x=this.x,
	 			y=this.y+sign(xy.dy)*1,							
					box=this.box},{0})
		
	if not collwallx then
			this.x+= collwally 
				and sign(xy.dx)*1
				or xy.dx
	end
	
	if not collwally then
			this.y+= collwallx 
					and sign(xy.dy)*1
					or xy.dy
	end	
end

p.draw = function(this)
	spr(this.s,this.x,this.y)
end

