function _init()
	es = {}
	local ol = {6,7,8,12,13,14}
	local il = {8,9,10,11,13,14}
	
	for o in all(ol) do
  for i in all(il) do
    add(es,newenemy(o*8,i*8,flr(rnd(2))))
  end
	end
end

function _update()
	p:update()
	foreach(es, function(e)
		e:update()
	end)
end

function _draw()
	cls()
	map()	
	p:draw()
	foreach(es, function(e)
		e:draw()
	end)
end
